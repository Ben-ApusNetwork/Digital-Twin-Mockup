{
  "name": "digital-twin-mvp-deployable",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "build": "npm run build:client && npm run build:server",
    "build:client": "mkdir -p dist/public && esbuild src/index.tsx --bundle --minify --outfile=dist/public/index.js && cp public/* dist/public/",
    "build:server": "mkdir -p dist && esbuild server/server.ts --bundle --minify --platform=node --format=esm --outfile=dist/server.js",
    "start": "node dist/server.js"
  },
  "dependencies": {
    "@google/genai": "^1.15.0",
    "express": "^4.19.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "esbuild": "^0.23.0",
    "typescript": "^5.5.3"
  }
}